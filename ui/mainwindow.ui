   <string notr="true">/* Allgemeine Stile */

/* Setzt die Standardschrift für alle Elemente */
* {
    font: 12pt &quot;Sitka Heading&quot;;
}

/* Stil für verschiedene Elemente */

/* QLabel */
QLabel {
    padding-left: 10px;
    color: #13213b; /* Textfarbe */
    background-color: none; /* Hintergrundfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
}

/* QTextEdit */
QTextEdit {
    color: #13213b; /* Textfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
    background-color: rgb(255, 255, 255); /* Hintergrundfarbe */
    font: 12pt &quot;Sitka Heading&quot;; /* Schriftart */
	text-align: left;
}

QPlainTextEdit {
    color: #13213b; /* Textfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
    background-color: rgb(255, 255, 255); /* Hintergrundfarbe */
    font: 12pt &quot;Sitka Heading&quot;; /* Schriftart */
	text-align: left;
}

/* Deaktivierte QTextEdit */
QPlainTextEdit::disabled {
    color: #13213b; /* Textfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
    background-color: rgb(144, 216, 216); /* Hintergrundfarbe */
}

/* QPushButton */
QPushButton {
    background-color: #e21839; /* Hintergrundfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
}

/* QPushButton beim Überfahren mit der Maus */
QPushButton::hover {
    background-color: rgb(170, 18, 43); /* Hintergrundfarbe */
    color: rgb(71, 71, 71); /* Textfarbe */
}

/* QPushButton beim Klicken */
QPushButton:pressed {
    background-color: rgb(0, 170, 255); /* Hintergrundfarbe */
    color: white; /* Textfarbe */
}

/* QFrame und QTabWidget-Panels (Inhalt) */

QFrame#frame0
{
    background-color: #13213b; /* Hintergrundfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
}



QFrame#frame010,
QFrame#frame011
{
    background-color: rgb(170, 255, 255); /* Hintergrundfarbe */
    border-radius: 5px; /* Abgerundete Ecken */
}

QTabWidget,
QTabWidget::pane 
{
    background-color: rgb(170, 255, 255); /* Hintergrundfarbe */
    border-top-right-radius: 5px; /* Abgerundete Ecken */
    border-bottom-left-radius: 5px; /* Abgerundete Ecken */
    border-bottom-right-radius: 5px; /* Abgerundete Ecken */
}

/* Registerkarten im TabBar und deren Ableitungen */
QTabBar::tab,
QTabBar::tab:selected,
QTabBar:tab:!selected {
    margin: 0.5px; /* Margin */
    width: 30px; /* Breite */
    min-height: 50px; /* Mindesthöhe */
    max-height: 250px; /* Maximale Höhe */
    padding-top: 10px; /* Padding nur oben */
    padding-bottom: 10px; /* Padding nur unten */
    border-top-left-radius: 5px; /* Obere linke Ecke abgerundet */
    border-bottom-left-radius: 5px; /* Untere linke Ecke abgerundet */
}

/* Ausgewählte Registerkarten */
QTabBar::tab:selected {
    background-color: rgb(0, 170, 255); /* Hintergrundfarbe */
    color: rgb(0, 0, 0); /* Textfarbe */
}

/* Nicht ausgewählte Registerkarten */
QTabBar:tab:!selected {
    background-color: rgb(210, 255, 255); /* Hintergrundfarbe */
    color: rgb(170, 170, 255); /* Textfarbe */
}

/* Hover-Effekt für nicht ausgewählte Registerkarten */
QTabBar:tab:!selected::hover {
    alignment: top; 
    background-color: rgb(0, 255, 255); 
    color: rgb(0, 85, 255); 
}

QTableWidget{
	background-color: rgb(220, 255, 255);
    border-radius: 5px; /* Abgerundete Ecken */
}

QTableWidget::item:alternate {
    background-color: rgb(240, 255, 255); /* Beispielhintergrundfarbe für alternative Zeilen */
}

QTableWidget::item:alternate::selected {
    background-color: rgb(200, 255, 255); /* Beispielhintergrundfarbe für alternative Zeilen */
	color: #13213b;
}

QTableWidget::item::selected {
    background-color: rgb(200, 255, 255); /* Beispielhintergrundfarbe für alternative Zeilen */
	color: #13213b;
}

QHeaderView::section{
	background-color: rgb(220, 255, 255);
}
